{"ast":null,"code":"export default {\n  name: 'AnimacionesView',\n\n  data() {\n    return {\n      files: [],\n      visible: true\n    };\n  },\n\n  methods: {\n    dragOver() {\n      const dropArea = document.getElementById(\"arch\");\n      const dragText = document.getElementById(\"dragText\");\n      dropArea.classList.add(\"active\");\n      dragText.textContent = \"Sulta para subir los archivos\";\n    },\n\n    dragLeave() {\n      const dropArea = document.getElementById(\"arch\");\n      const dragText = document.getElementById(\"dragText\");\n      dropArea.classList.remove(\"active\");\n      dragText.textContent = \"Arrastra tu archivo\";\n    },\n\n    dropPDF(event) {\n      var archivo = event.dataTransfer.files;\n      console.log(archivo);\n      const dropArea = document.getElementById(\"arch\");\n      const dragText = document.getElementById(\"dragText\");\n      dropArea.classList.remove(\"active\");\n      dragText.textContent = \"Arrastra tu archivo\";\n    },\n\n    clickPDF() {\n      const input = document.getElementById(\"inputFile\");\n      input.click();\n    },\n\n    showFiles() {\n      var files = document.getElementById(\"inputFile\").files;\n      var ok = true;\n\n      if (files.length != undefined) {\n        for (const file of files) {\n          ok = this.processFile(file);\n\n          if (!ok) {\n            break;\n          }\n        }\n      }\n\n      if (ok) {\n        alert(\"Aceptado\");\n      } else {\n        alert(\"Solo png/jpg/pdf\");\n        const input = document.getElementById(\"inputFile\");\n        input.value = \"\";\n      }\n    },\n\n    processFile(file) {\n      const docType = file.type;\n      const validExtensons = ['image/png', 'image/jpg', 'image/jpeg', 'application/pdf'];\n\n      if (!validExtensons.includes(docType)) {\n        return false;\n      }\n\n      return true;\n    },\n\n    inputChange() {\n      const dropArea = document.getElementById(\"arch\");\n      dropArea.classList.add(\"active\");\n      this.showFiles();\n      dropArea.classList.remove(\"active\");\n      this.files = document.getElementById(\"inputFile\").files;\n      this.visible = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4CA,eAAe;AACbA,MAAI,EAAE,iBADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,EADF;AAELC,aAAO,EAAE;AAFJ,KAAP;AAID,GAPY;;AAQbC,SAAO,EAAE;AACPC,YAAQ,GAAE;AACR,YAAMC,QAAO,GAAIC,QAAQ,CAACC,cAAT,CAAwB,MAAxB,CAAjB;AACA,YAAMC,QAAO,GAAIF,QAAQ,CAACC,cAAT,CAAwB,UAAxB,CAAjB;AACAF,cAAQ,CAACI,SAAT,CAAmBC,GAAnB,CAAuB,QAAvB;AACAF,cAAQ,CAACG,WAAT,GAAuB,+BAAvB;AACD,KANM;;AAOPC,aAAS,GAAE;AACT,YAAMP,QAAO,GAAIC,QAAQ,CAACC,cAAT,CAAwB,MAAxB,CAAjB;AACA,YAAMC,QAAO,GAAIF,QAAQ,CAACC,cAAT,CAAwB,UAAxB,CAAjB;AACAF,cAAQ,CAACI,SAAT,CAAmBI,MAAnB,CAA0B,QAA1B;AACAL,cAAQ,CAACG,WAAT,GAAuB,qBAAvB;AACD,KAZM;;AAaPG,WAAO,CAACC,KAAD,EAAQ;AACb,UAAIC,OAAM,GAAID,KAAK,CAACE,YAAN,CAAmBhB,KAAjC;AACAiB,aAAO,CAACC,GAAR,CAAYH,OAAZ;AACA,YAAMX,QAAO,GAAIC,QAAQ,CAACC,cAAT,CAAwB,MAAxB,CAAjB;AACA,YAAMC,QAAO,GAAIF,QAAQ,CAACC,cAAT,CAAwB,UAAxB,CAAjB;AACAF,cAAQ,CAACI,SAAT,CAAmBI,MAAnB,CAA0B,QAA1B;AACAL,cAAQ,CAACG,WAAT,GAAuB,qBAAvB;AACD,KApBM;;AAqBPS,YAAQ,GAAG;AACT,YAAMC,KAAI,GAAIf,QAAQ,CAACC,cAAT,CAAwB,WAAxB,CAAd;AACAc,WAAK,CAACC,KAAN;AACD,KAxBM;;AAyBPC,aAAS,GAAE;AACT,UAAItB,KAAI,GAAIK,QAAQ,CAACC,cAAT,CAAwB,WAAxB,EAAqCN,KAAjD;AACA,UAAIuB,EAAC,GAAI,IAAT;;AACA,UAAGvB,KAAK,CAACwB,MAAN,IAAgBC,SAAnB,EAA6B;AAC3B,aAAI,MAAMC,IAAV,IAAkB1B,KAAlB,EAAwB;AACtBuB,YAAC,GAAI,KAAKI,WAAL,CAAiBD,IAAjB,CAAL;;AACA,cAAG,CAACH,EAAJ,EAAO;AACL;AACF;AACF;AACF;;AACA,UAAGA,EAAH,EAAM;AACJK,aAAK,CAAC,UAAD,CAAL;AACF,OAFA,MAGI;AACFA,aAAK,CAAC,kBAAD,CAAL;AACA,cAAMR,KAAI,GAAIf,QAAQ,CAACC,cAAT,CAAwB,WAAxB,CAAd;AACAc,aAAK,CAACS,KAAN,GAAc,EAAd;AACF;AACD,KA5CM;;AA6CPF,eAAW,CAACD,IAAD,EAAM;AACf,YAAMI,OAAM,GAAIJ,IAAI,CAACK,IAArB;AACA,YAAMC,cAAa,GAAI,CAAC,WAAD,EAAc,WAAd,EAA0B,YAA1B,EAAwC,iBAAxC,CAAvB;;AACA,UAAG,CAACA,cAAc,CAACC,QAAf,CAAwBH,OAAxB,CAAJ,EAAqC;AACnC,eAAO,KAAP;AACF;;AACA,aAAO,IAAP;AACD,KApDM;;AAqDPI,eAAW,GAAE;AACX,YAAM9B,QAAO,