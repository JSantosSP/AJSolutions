{"ast":null,"code":"export default {\n  name: 'AnimacionesView',\n\n  data() {\n    return {};\n  },\n\n  methods: {\n    dropPDF() {\n      //const dropArea = document.querySelector(\".arch\");\n      //const dragText = dropArea.querySelector(\"h1\");\n      //const button = dropArea.querySelector(\"button\");\n      //const imput = dropArea.querySelector(\"imput\");\n\n      /*button.addEventListener(\"click\", (e) => {\r\n          console.log(e);\r\n      })*/\n      function handleFileSelect(evt) {\n        evt.stopPropagation();\n        evt.preventDefault();\n        var files = evt.dataTransfer.files; // FileList object.\n        // files is a FileList of File objects. List some properties.\n\n        var output = [];\n\n        for (var i = 0, f; f == files[i]; i++) {\n          output.push('<li><strong>', escape(f.name), '</strong> (', f.type || 'n/a', ') - ', f.size, ' bytes, last modified: ', f.lastModifiedDate.toLocaleDateString(), '</li>');\n        }\n\n        document.getElementById('list').innerHTML = '<ul>' + output.join('') + '</ul>';\n      }\n\n      function handleDragOver(evt) {\n        evt.stopPropagation();\n        evt.preventDefault();\n        evt.dataTransfer.dropEffect = 'copy'; // Explicitly show this is a copy.\n      } // Setup the dnd listeners.\n\n\n      var dropZone = document.getElementById('drop_zone');\n      dropZone.addEventListener('dragover', handleDragOver, false);\n      dropZone.addEventListener('drop', handleFileSelect, false);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsCA,eAAe;AACbA,MAAI,EAAE,iBADO;;AAEbC,MAAI,GAAG;AACL,WAAO,EAAP;AAGD,GANY;;AAObC,SAAO,EAAE;AACPC,WAAO,GAAG;AACR;AACA;AACA;AACA;;AAEA;;;AAGA,eAASC,gBAAT,CAA0BC,GAA1B,EAA+B;AAC7BA,WAAG,CAACC,eAAJ;AACAD,WAAG,CAACE,cAAJ;AAEA,YAAIC,KAAI,GAAIH,GAAG,CAACI,YAAJ,CAAiBD,KAA7B,CAJ6B,CAIO;AAEpC;;AACA,YAAIE,MAAK,GAAI,EAAb;;AACA,aAAK,IAAIC,IAAI,CAAR,EAAWC,CAAhB,EAAmBA,KAAKJ,KAAK,CAACG,CAAD,CAA7B,EAAkCA,CAAC,EAAnC,EAAuC;AACrCD,gBAAM,CAACG,IAAP,CAAY,cAAZ,EAA4